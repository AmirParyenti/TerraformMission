pipeline {
    agent any
    stages {
        stage('Test SSH Connection') {
            steps {
                script {
                    sshagent(['ec2-deployer-key	']) {
                        sh "ssh -o StrictHostKeyChecking=no ec2-user@3.147.103.73 echo 'SSH connection successful'"
                    }
                }
            }
        }
    }
}
